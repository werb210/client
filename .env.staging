# Staging Environment Configuration
# This file allows QA to easily toggle between development and live staff API

# Staff Backend API Configuration
VITE_API_BASE_URL=https://staffportal.replit.app/api

# SignNow Integration
SIGNNOW_REDIRECT_URL=https://clientportal.replit.app/step6-signature

# Environment Identifier
NODE_ENV=staging

# Client Application Configuration
CLIENT_APP_SHARED_TOKEN=your-shared-token-here

# Cache and Sync Configuration
ENABLE_BACKGROUND_SYNC=true
SYNC_RETRY_INTERVAL=1800000

# Debug Configuration (staging specific)
ENABLE_DEBUG_LOGGING=true
ENABLE_API_DIAGNOSTICS=true